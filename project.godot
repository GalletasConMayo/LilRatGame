; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="LilRatGame"
run/main_scene="res://Scenes/game.tscn"
config/features=PackedStringArray("4.2", "Forward Plus")
run/max_fps=144
config/icon="res://icon.svg"

[autoload]

global_variables="*res://Scripts/global_variables.gd"
PhantomCameraManager="*res://addons/phantom_camera/scripts/managers/phantom_camera_manager.gd"
camera_control="*res://Scripts/camera_control.gd"

[display]

window/size/viewport_width=640
window/size/viewport_height=360
window/size/window_width_override=1280
window/size/window_height_override=720
window/stretch/mode="viewport"
window/snap/snap_2d_transforms_to_pixel=true
window/snap/snap_2d_vertices_to_pixel=true

[dotnet]

project/assembly_name="PlaT"

[editor_plugins]

enabled=PackedStringArray("res://addons/phantom_camera/plugin.cfg")

[importer_defaults]

animation_library={
"animation/fps": 60.0
}

